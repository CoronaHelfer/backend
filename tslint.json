{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "avoid-template"
    ],
    "object-literal-sort-keys": false,
    "arrow-parens": false,
    "ordered-imports": false,
    "no-shadowed-variable": false
  },
  "rulesDirectory": []
}
